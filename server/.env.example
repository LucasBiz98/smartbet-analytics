# Configuración del Servidor SmartBet Analytics
# Copiar este archivo a .env y completar con tus valores

# Puerto del servidor
PORT=3001

# Database PostgreSQL (Railway o local)
DATABASE_URL=postgresql://postgres:password@localhost:5432/smartbet

# Configuración de Playwright
PLAYWRIGHT_BROWSERS_PATH=0

# Configuración de Logging
LOG_LEVEL=info

# URL de la casa de apuestas (para Bet Assist)
BOOKMAKER_URL=https://www.bet365.com

#密钥 para servicios externos (si son necesarios)
EXTERNAL_API_KEY=
